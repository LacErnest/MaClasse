<body>
  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
      <div class="content-wrapper d-flex align-items-center auth">
        <div class="row w-100">
          <div class="col-lg-8 mx-auto">
            <div class="row">
              <div class="col-lg-8 bg-white">
                <div class="auth-form-light text-left p-5">
                  <h2>Créer Votre Compte</h2>
                  <form class="pt-4" action="{{ path('register') }}" method="post">
                      <div class="form-group">
                        {{ form_errors(form.nom) }}
            {{ form_row(form.nom) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.prenom) }}
            {{ form_row(form.prenom) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.username) }}
            {{ form_row(form.username) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.password) }}
            {{ form_row(form.password) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.email) }}
            {{ form_row(form.email) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.phone) }}
            {{ form_row(form.phone) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="form-group">
                        {{ form_errors(form.phone2) }}
            {{ form_row(form.phone2) }}
                        <i class="mdi mdi-account"></i>
                      </div>
                      <div class="mt-5">
                        {{ form_row(form._token) }}
                        <button class="btn btn-block btn-success btn-lg" type="submit">Enregistrer</button>
                      </div>
                      <div class="mt-2 w-75 mx-auto">
                        <div class="form-check form-check-flat">
                          <label class="form-check-label">
                            <input type="checkbox" class="form-check-input">
                            J'accepte les termes et conditions
                          </label>
                        </div>
                      </div>
                      <div class="mt-2 text-center">
                        <a href="{{ path('login') }}" class="auth-link text-black">Déja un Compte? <span class="font-weight-medium">Se Connecter</span></a>
                      </div>                  
                  </form>
                </div>
              </div>
              <div class="col-lg-4 register-half-bg d-flex flex-row">
                <p class="text-white font-weight-medium text-center flex-grow align-self-end">Copyright &copy; 2019  All rights reserved.</p>
              </div>
            </div>
          </div>
        </div>
